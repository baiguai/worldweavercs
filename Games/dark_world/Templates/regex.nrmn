    {attrib, tags=north, output=\b(?:north|n|go north)\b }
    {attrib, tags=ne_up, output=\b(?:go\s+)?(?:northeast|ne|up(?:\s+stairs)?)\b }
    {attrib, tags=east, output=^(e|east|go east)$ }
    {attrib, tags=south, output=^(go\s+south|south|s)$ }
