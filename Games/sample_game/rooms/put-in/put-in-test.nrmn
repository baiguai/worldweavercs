{room, key=put_in_test_room, name=Put In Test Room
    {enter_message
        output={@
This is a room for testing functionality.
        }

        {set, logic=[game]((game_started)), output=true }
    }

    {look
        {message
            output={@
You are in the test room.
            }
        }
    }

    {navigation, tags=look|enter_message
        {message
            output={@
You can go home, to the 'home room'.
            }
        }
    }

    {inject, logic=look.inj }

--------------------------------------------------------------------------------

    {input, syntax=(?i)\b(?:go\s+to\s+)?(?:the\s+)?home\s+room\b
        {move, logic=home, tags=[player], output=You return to the testing home room. }
    }

    {inject, logic=./thebox.inj }
    {inject, logic=./thekey.inj }

}
