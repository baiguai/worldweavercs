{
    "commands" : [
        {
            "pattern" : "\\b(list games)\\b",
            "method" : "DoListGames",
            "during_game" : "false"
        },
        {
            "pattern" : "\\b(menu)\\b",
            "method" : "DoMenu",
            "during_game" : "false"
        },
        {
            "pattern" : "\\b(play *)\\b",
            "method" : "DoPlayGame",
            "during_game" : "false"
        },
        {
            "pattern" : "\\b(resume *)\\b",
            "method" : "DoResumeGame",
            "during_game" : "false"
        },
        {
            "pattern" : "\\b(set player name *)\\b",
            "method" : "DoSetPlayerName",
            "during_game" : "true"
        },
        {
            "pattern" : "\\b(time)\\b",
            "method" : "DoTime",
            "during_game" : "true"
        },
        {
            "pattern" : "^:h(\\s\\d+)?$",
            "method" : "DoHistory",
            "during_game" : "true"
        },
        {
            "pattern" : "(?i)^noteadd.*",
            "method" : "DoNoteAdd",
            "during_game" : "true"
        },
        {
            "pattern" : "^notedelete$",
            "method" : "DoNoteDelete",
            "during_game" : "true"
        },
        {
            "pattern" : "^listnotes$",
            "method" : "DoNotesList",
            "during_game" : "true"
        },
        {
            "pattern" : "^note.*",
            "method" : "DoNoteView",
            "during_game" : "true"
        }
    ]
}
