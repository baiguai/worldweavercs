{
    "commands" : [
        {
            "pattern" : "\\b(list games)\\b",
            "method" : "DoListGames",
            "during_game" : "false"
        },
        {
            "pattern" : "\\b(play *)\\b",
            "method" : "DoPlayGame",
            "during_game" : "false"
        },
        {
            "pattern" : "\\b(resume *)\\b",
            "method" : "DoResumeGame",
            "during_game" : "false"
        },
        {
            "pattern" : "\\b(set player name *)\\b",
            "method" : "DoSetPlayerName",
            "during_game" : "false"
        },
        {
            "pattern" : "\\b(menu)\\b",
            "method" : "DoMenu",
            "during_game" : "false"
        },
        {
            "pattern" : "\\b(quit)\\b",
            "method" : "DoQuit",
            "during_game" : "true"
        }
    ]
}
